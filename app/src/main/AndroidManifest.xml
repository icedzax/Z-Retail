<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zubb.jannarongj.z_retail">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UnlockActivity"></activity>
        <activity
            android:name=".NoShipActivity"
            android:label="@string/title_activity_no_ship"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".QueueList"
            android:label="@string/title_activity_queue_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Stock"
            android:label="@string/title_activity_stock"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".QrCam"
            android:label="@string/title_activity_qr_cam"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CompleteDo"
            android:label="@string/title_activity_complete_do"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >

        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CheckVersion"
            android:label="@string/title_activity_check_version"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RetailList"
            android:label="@string/title_activity_retail_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ListDo"
            android:label="@string/title_activity_list_do"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddProducts"
            android:label="@string/title_activity_add_products"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ar_list"
            android:label="@string/title_activity_ar_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ar_list_back"
            android:label="@string/title_activity_ar_list"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>